<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input id="postTitle" class="form-control" type="text" name="postTitle" autocomplete="off">
  </div>
  <div class="form-group">
    <label for="postBody">Post</label>
    <textarea id="postBody" class="form-control" rows="5" type="text" name="postBody"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
